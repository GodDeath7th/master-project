<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="homePane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomeController">
   <children>
      <ImageView fx:id="headImage" fitHeight="166.0" fitWidth="1280.0">
         <image>
            <Image url="@../../../sun-shine-head.jpg" />
         </image>
      </ImageView>
      <JFXButton fx:id="addButton" buttonType="RAISED" layoutX="923.0" layoutY="59.0" onAction="#goAddPatient" prefHeight="48.0" prefWidth="157.0" ripplerFill="BLACK" style="-fx-background-color: lightgreen; -fx-background-radius: 15;" text="NEW" textFill="WHITE">
         <font>
            <Font name="Calibri Light" size="24.0" />
         </font>
      </JFXButton>
      <TextField fx:id="searchBar" layoutX="151.0" layoutY="59.0" prefHeight="48.0" prefWidth="753.0" style="-fx-background-color: white; -fx-background-radius: 20; -fx-border-color: #ffa13e; -fx-border-radius: 20;" />
      <ScrollPane fx:id="patientListPane" hbarPolicy="NEVER" layoutX="100.0" layoutY="200.0" prefHeight="520.0" prefWidth="1150.0" style="-fx-background-color: transparent;">
         <content>
            <VBox fx:id="patientListBox" prefWidth="1100.0">
               <opaqueInsets>
                  <Insets bottom="20.0" />
               </opaqueInsets>
            </VBox>
         </content>
      </ScrollPane>
      <ImageView fx:id="tipImage1" fitHeight="100.0" fitWidth="100.0" layoutX="540.0" layoutY="330.0">
         <image>
            <Image url="@../image/boy.png" />
         </image>
      </ImageView>
      <ImageView fx:id="tipImage2" fitHeight="100.0" fitWidth="100.0" layoutX="640.0" layoutY="330.0">
         <image>
            <Image url="@../image/girl.png" />
         </image>
      </ImageView>
      <Label fx:id="tipLabel" layoutX="533.0" layoutY="450.0" text="NO PATIENT" textFill="#1acdaf">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <ImageView fx:id="backImage" fitHeight="50.0" fitWidth="50.0" layoutX="25.0" layoutY="14.0">
         <image>
            <Image url="@../../../back.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
