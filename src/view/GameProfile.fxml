<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="gameProfilePane" prefHeight="40.0" prefWidth="250.0" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GameProfileController">
   <children>
      <ImageView fx:id="gameProfileImage" fitHeight="40.0" fitWidth="40.0" layoutX="10.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../basketball.png" />
         </image>
      </ImageView>
      <Rectangle fx:id="levelBar" arcHeight="5.0" arcWidth="5.0" fill="DARKGRAY" height="5.0" layoutX="65.0" layoutY="12.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="140.0" />
      <Rectangle fx:id="repeatBar" arcHeight="5.0" arcWidth="5.0" fill="DARKGRAY" height="5.0" layoutX="65.0" layoutY="24.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="140.0" />
      <Label fx:id="level" layoutX="220.0" layoutY="7.0" text="5" />
      <Label fx:id="repeat" layoutX="220.0" layoutY="19.0" text="5" />
   </children>
</AnchorPane>
